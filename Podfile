project 'Demo.xcodeproj'

platform :ios, '11.0'
use_frameworks!

target 'DemoObjC' do

    pod "IQDropDownTextField"
    pod "IQKeyboardManager", :path => "."

end

target 'DemoSwift' do

    pod 'SwiftLint'
    pod "IQDropDownTextFieldSwift"
    pod "IQKeyboardManagerSwift", :path => "."

    pod "RSKPlaceholderTextView"
end


target 'DemoExtension' do

    pod 'SwiftLint'
    pod "IQKeyboardManagerSwift", :path => "."

end
